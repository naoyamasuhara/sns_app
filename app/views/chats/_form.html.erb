<%= form_for [@community, @chat] do |f| %>
  <div class="form_gruop">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control chat__input-title' %>
  </div>
  <div class="form_gruop">
    <%= f.label :description %>
    <%= f.text_area :description, class: 'form-control chat__input-description' %>
  </div>
  <div class="chat__input-submit">
    <%= f.submit '作成する', class: "btn btn-success" %>
  </div>
<% end %>